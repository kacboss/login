<!doctype html>
<html lang="{{ site.language }}">
<head>


  <meta charset="UTF-8">
  <title>{{ wp_title ? wp_title : site.name }}</title>

  <meta name="viewport" content="width=device-width, user-scalable=yes, initial-scale=1. maximum-scale=5, minimum-scale=1">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">

  <link rel="icon" href="{{ theme.link }}/favicon/favicon.ico" sizes="16x16" />

  {{ options.additionals.head }}
  {{ function('wp_head') }}

  <link
  rel="stylesheet"
  href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css"
/>  

<script src="https://cdn.jsdelivr.net/npm/inview@0.0.15/inview.min.js"></script>

<script src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js"></script>

  <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.2/gsap.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.2/ScrollTrigger.min.js"></script>
</head>

<body>

{{ options.additionals.body }}

<header class="header">
  <div class="row">
    <div class="column">
      <a class="header__logo" href="{{ site.link }}">
        {% include 'svg/Logintrade.svg' %}
      </a>

      <ul class="header__menu">
        <li class="header__menu__item active">
          <a href="">Start</a>
        </li>
        <li class="header__menu__item">
          <a href="">About us</a>
        </li>
        <li class="header__menu__item">
          <a href="">Customer</a>
        </li>
      </ul>

      <div class="header__menu--right">
        <a href="" class="sign-in">{% include 'svg/person.svg' %} Sign in</a>
        <a href="" class="btn">Book a demo {% include 'svg/arrow-right.svg' %}</a>
      </div>

    </div>
  </div>
</header>
